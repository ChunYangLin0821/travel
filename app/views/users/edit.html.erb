<%= form_for @user, :url => user_path(@user), :method => :patch do |f| %>
<%= f.label :name, "Name" %>
<%= f.text_field :name %>

<%= f.label :email, "Email" %>
<%= f.text_field :email %>

<%= f.label :age, "Age" %>
<%= f.text_field :age %>

<%= f.label :from, "From" %>
<%= f.text_field :from %>

<%= f.submit "Update" %>
<% end %>
