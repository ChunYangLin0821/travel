<%= form_for @user, :url => { :controller => 'users', :action => 'create' } do |f| %>
<%= f.label :name, "Name" %>
<%= f.text_field :name %>

<%= f.label :email, "Email" %>
<%= f.text_field :email %>

<%= f.label :age, "Age" %>
<%= f.text_field :age %>

<%= f.label :from, "From" %>
<%= f.text_field :from %>

<%= f.submit "Create" %>
<% end %>
