<ul>
<% @posts.each do |post| %>
<li>
<%= post.title %>
<%= link_to 'show', user_post_path(@user, post) %>
<%= link_to 'edit', edit_user_post_path(@user, post) %>
<%= link_to 'destroy', user_post_path(@user, post), :method => :delete %>
</li>
<% end %>

</ul>

<%= link_to 'new post', new_user_post_path(@user) %>
